<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_006_5"
    android:clipToPadding="false"
    android:fitsSystemWindows="true"
    android:paddingBottom="@dimen/space_20"
    tools:ignore="WebViewLayout">

    <include
        android:id="@+id/head_id"
        layout="@layout/head_layout"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/tv3_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/space_30"
        android:gravity="center"
        android:text="@string/text9"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <WebView
        android:id="@+id/web_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_290"
        android:layout_marginStart="@dimen/space_20"
        android:layout_marginEnd="@dimen/space_20"
        android:layout_marginBottom="@dimen/space_10"
        app:layout_constraintTop_toBottomOf="@id/head_id" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl1_id"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_50"
        android:layout_marginStart="@dimen/space_20"
        android:layout_marginTop="@dimen/sp_10"
        android:layout_marginEnd="@dimen/space_20"
        app:layout_constraintTop_toBottomOf="@id/web_view">

        <TextView
            android:id="@+id/tv1_id"
            android:layout_width="@dimen/space_100"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_white_5_"
            android:gravity="center"
            android:padding="@dimen/space_8"
            android:text="@string/text150"
            android:textColor="@color/f006"
            android:textSize="@dimen/sp_12"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv2_id"
            android:layout_width="@dimen/space_100"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_f2cd00_5"
            android:gravity="center"
            android:padding="@dimen/space_8"
            android:text="@string/text149"
            android:textColor="@color/f006"
            android:textSize="@dimen/sp_12"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>